{
  "name": "manoa-link",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.20.13",
    "@fullcalendar/core": "^6.1.6",
    "@fullcalendar/daygrid": "^6.1.6",
    "@fullcalendar/react": "^6.1.6",
    "bootstrap": "^5.2.3",
    "classnames": "^2.3.2",
    "core-js": "^3.28.0",
    "graphql": "^16.6.0",
    "meteor-node-stubs": "^1.2.5",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-bootstrap-icons": "^1.10.2",
    "react-dom": "^18.2.0",
    "react-router": "^6.8.1",
    "react-router-dom": "^6.8.1",
    "react-select": "^5.7.2",
    "simpl-schema": "^3.4.1",
    "sweetalert": "^2.1.2",
    "uniforms": "^3.10.2",
    "uniforms-bootstrap5": "^3.10.2",
    "uniforms-bridge-simple-schema-2": "^3.10.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-meteor": "^7.3.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-utils": "^3.0.0",
    "prettier": "^2.8.7",
    "sass": "^1.61.0",
    "testcafe": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "build": "meteor reset && sass client/style.scss client/style.css",
    "dev": "concurrently -n \"sass,meteor\" \"sass client/style.scss client/style.css --watch\" \"meteor npm run start\"",
    "prestart": "meteor npm run build",
    "start": "meteor --no-release-check --exclude-archs web.browser.legacy,web.cordova --settings ../config/settings.development.json",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts ./imports && eslint --ext .js,.ts ./tests",
    "lint-fix": "eslint --quiet --fix --ext .jsx,.js,.tsx,.ts ./imports && eslint --quiet --fix --ext .js,.ts ./tests",
    "testcafe": "testcafe chrome tests/*.testcafe.ts",
    "testcafe-single": "testcafe chrome tests/*.testcafe.ts -t \"Test that signin and signout work\"",
    "testcafe-ci": "testcafe chrome:headless tests/*.testcafe.ts -q",
    "test-all": "meteor reset && meteor npm run lint && meteor npm run unimported && meteor npm run testcafe-ci",
    "unimported": "npx unimported"
  }
}
